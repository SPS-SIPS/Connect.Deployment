{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Information",
                "System": "Information"
            }
        },
        "WriteTo:Console": {
            "Name": "Console",
            "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
                "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
            }
        },
        "WriteTo:File": {
            "Name": "File",
            "Args": {
                "path": "logs/log.log",
                "rollingInterval": "Month",
                "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName"
        ],
        "Destructure": [],
        "Properties": {
            "Application": "SPS System Logs"
        }
    },
    "ConnectionStrings": {
        "db": "Host=localhost;Database=SIPS.Connect.DB;Include Error Detail=True;Username=postgres;Password={{PASSWORD}};"
    },
    "AllowedHosts": "*",
    "Core": {
        "BaseUrl": "http://10.128.200.244:5004/api",
        "PublicKeysRepUrl": "/v1/Certificates/Download",
        "LoginEndpoint": "/v1/auth/login",
        "Username": "<Username>",
        "Password": "<Password>",
        "BIC": "ZKBASOS0",
        "SAFExpression": "*/10 * * * *",
        "SAFPage": 40,
        "SAFTimeZoneInfo": "Utc",
        "SAFMaxRetries": 10
    },
    "Xades": {
        "CertificatePath": "/certs/certificate.pem",
        "PrivateKeyPath": "/certs/private.pem",
        "PrivateKeyPassphrase": "",
        "ChainPath": "/certs/chain.pem",
        "Algorithms": [
            "SHA1withRSA",
            "SHA256withRSA",
            "SHA384withRSA",
            "SHA512withRSA"
        ],
        "VerificationWindowMinutes": 1000,
        "BIC": "ZKBASOS0",
        "WithoutPKI": true,
        "BaseDN": "CN=sps issuing CA, DC=sps, DC=local"
    },
    "ISO20022": {
        "Verification": "http://sips-corebank:8080/api/CB/Verify",
        "Transfer": "http://sips-corebank:8080/api/CB/Transfer",
        "Return": "http://sips-corebank:8080/api/CB/Return",
        "Status": "http://sips-corebank:8080/api/CB/Status",
        "SIPS": "http://10.128.0.108:9001/v1/iso20022/incoming",
        "CompletionNotification": "http://sips-corebank:8080/api/CB/CompletionNotification",
        "BIC": "ZKBASOS0",
        "Agent": "ZKBASOS0",
        "Key": "Very_Secret_Key",
        "Secret": "Very_Secret_Secret"
    },
    "Emv": {
        "AcquirerId": "0012",
        "FIType": "01",
        "FIName": "Zirat Bank",
        "Version": "01",
        "CountryCode": "SO",
        "Tags": {
            "MerchantIdentifier": 26,
            "AcquirerTag": 1,
            "MerchantIdTag": 44
        }
    },
    "Keycloak": {
        "Realm": {
            "Host": "localhost:8443",
            "Protocol": "https",
            "Name": "mgt",
            "Audience": "account"
        }
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "https://0.0.0.0:8080"
            },
            "Https": {
                "Url": "https://0.0.0.0:443",
                "Certificate": {
                    "Path": "/certs/sips-connect.pfx",
                    "Password": "YOUR_PASSWORD"
                }
            }
        }
    },
    "CorsPolicies": {
        "Origins": [
            "https://localhost:3000"
        ]
    },
    "ApiKeys": [
    {
      "Key": "123",
      "Secret": "123",
      "Name": "Test"
    }
  ]
}